{
  "name": "extensions",
  "private": true,
  "type": "module",
  "version": "2.0.2",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "wireit",
    "type:check": "wireit"
  },
  "dependencies": {
    "@blockly/field-grid-dropdown": "5.0.12",
    "@blockly/field-slider": "7.0.12",
    "blockly": "catalog:",
    "hono": "catalog:",
    "uuidv7": "1.0.2"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:"
  },
  "wireit": {
    "build": {
      "command": "tsc -p tsconfig.build.json",
      "files": ["src", "tsconfig.build.json", "package.json"],
      "output": ["dist"]
    },
    "type:check": {
      "command": "tsc -p tsconfig.json --noEmit",
      "files": ["src", "tsconfig.json", "package.json"],
      "output": ["dist"]
    }
  }
}
