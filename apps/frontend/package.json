    "start": {
      "command": "cross-env NODE_ENV=production vite preview",
      "dependencies": ["../../packages/extensions:build", "../backend:build"]
    },
    "type:check": {
      "command": "tsc --noEmit",
      "files": ["src", "package.json", "tsconfig.json"],
      "output": [],
