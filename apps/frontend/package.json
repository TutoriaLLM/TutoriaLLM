  "wireit": {
    "dev": {
      "command": "vite",
      "dependencies": ["../../packages/extensions:build", "../backend:build"],
      "service": {
        "readyWhen": {
          "lineMatches": "Local:   http://localhost:\\d+/"
        }
      }
    },
    "build": {
      "command": "vite build",
      "files": ["src", "tsconfig.json", "package.json"],
      "output": ["dist"],
