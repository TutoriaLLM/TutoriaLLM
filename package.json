{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build",
    "reset-user": "tsx src/server/db/users.ts --reset-credentials",
    "reset-tutorials": "tsx src/server/db/tutorials.ts --reset-tutorials",
    "lint": "biome lint ./src/",
    "format": "biome format ./src/",
    "check": "biome check ./src/",
    "lint:fix": "biome lint --apply ./src/",
    "format:fix": "biome format ./src/ --write",
    "check:fix": "biome check --apply ./src/"
  },
  "dependencies": {
    "@blockly/continuous-toolbox": "^6.0.1",
    "@langchain/openai": "^0.2.0",
    "@lucia-auth/adapter-sqlite": "^3.0.1",
    "@marp-team/marpit": "^3.0.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@types/js-yaml": "^4.0.9",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.6.0",
    "blockly": "^11.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "i18next": "^23.11.5",
    "i18next-fs-backend": "^2.3.1",
    "input-otp": "^1.2.4",
    "jotai": "^2.8.2",
    "js-yaml": "^4.1.0",
    "kysely": "^0.27.3",
    "langchain": "^0.2.6",
    "level": "^8.0.1",
    "lucia": "^3.2.0",
    "lucide-react": "^0.381.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-resizable-panels": "^2.0.19",
    "react-router-dom": "^6.23.1",
    "tsx": "^4.3.0",
    "typescript": "^5.3.2",
    "uuid": "^9.0.1",
    "vectra": "^0.7.6",
    "vite-express": "*",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.10",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/node": "^20.9.3",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.19",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "vite": "^5.0.2"
  }
}
